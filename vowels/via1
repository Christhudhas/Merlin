import java.util.Scanner;

public class Viva
{
    public static void main(String[] args)
    {
        Scanner scan = new Scanner(System.in);

       
        String[] v = new String[4]; 

        
        System.out.println("Enter a value for index 1:");
        v[1] = scan.nextLine();
       
        System.out.println("Value at index 1: " + v[1]);
    }
}
